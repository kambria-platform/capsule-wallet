  "main": "index.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "build": "npm run clean && webpack --mode=production",
    "clean": "rm -rf ./index.js && rm -rf ./node_modules/capsule-wallet",
    "start": "nodemon --exec 'npm run dev && npm run serve'",
    "serve": "HTTPS=true REACT_APP_ENV=development react-scripts start",
    "dev": "webpack --mode=development && npm run fake-module",
    "fake-module": "mkdir -p ./node_modules/capsule-wallet && cp -R ./index.js ./node_modules/capsule-wallet",
    "css": "sass ./wallet/skin/static/style/index.scss ./wallet/skin/static/style/index.css --style compressed",
    "test": ""
  },
